# Wanderer Finance Environment Configuration
# Copy this file to .env and fill in your actual API keys

# Alpha Vantage API Key (for stock data and insider trading)
# Get your free key at: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# News Data API Key (for news sentiment analysis)
# Get your key at: https://newsdata.io/
NEWS_DATA_API_KEY=your_news_data_api_key_here

# Groq API Key (for LLM access)
# Get your key at: https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here

# LangSmith Configuration (optional, for LLM tracing)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=wanderer-finance

# Database Configuration (optional overrides)
# DATABASE_PATH=main.db
# DATABASE_TABLE=data

# Application Configuration (optional overrides)
# LOG_LEVEL=INFO
# GRADIO_SHARE=false
# GRADIO_DEBUG=false

# Trading Configuration (optional overrides)
# MAX_ARTICLES_PER_STOCK=2
# MARKET_CHECK_ENABLED=true
